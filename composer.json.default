{
  "require": {
    "craftcms/aws-s3": "^2.0.3",
    "craftcms/cms": "^4.4.7",
    "craftcms/redactor": "^3.0.4",
    "diginov/craft-sentry-logger": "^4.1.2",
    "mmikkel/cp-field-inspect": "^1.4.4",
    "nystudio107/craft-retour": "^4.1.12",
    "nystudio107/craft-seomatic": "^4.0.23",
    "percipioglobal/craft-colour-swatches": "^4.3.0",
    "putyourlightson/craft-amazon-ses": "^2.0.1",
    "sebastianlenz/linkfield": "^2.1.5",
    "servd/craft-asset-storage": "^3.3.0",
    "spicyweb/craft-embedded-assets": "^3.1.4",
    "verbb/formie": "^2.0.26",
    "verbb/image-resizer": "^3.0.4",
    "verbb/super-table": "^3.0.8.1",
    "vlucas/phpdotenv": "^5.5.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.1",
    "nystudio107/craft-autocomplete": "^1.11.0",
    "yiisoft/yii2-shell": "^2.0.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "php-http/discovery": false,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    }
  },
  "scripts": {
    "post-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
