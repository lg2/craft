{
  "name": "lg2/craft",
  "description": "Headless Craft CMS Starter",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "0BSD",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "craftcms/aws-s3": "^2.0.3",
    "craftcms/cms": "^4.4.5",
    "craftcms/redactor": "^3.0.4",
    "diginov/craft-sentry-logger": "^4.1.1",
    "mmikkel/cp-field-inspect": "^1.4.4",
    "nystudio107/craft-retour": "^4.1.11",
    "nystudio107/craft-seomatic": "^4.0.23",
    "percipioglobal/craft-colour-swatches": "^4.3.0",
    "putyourlightson/craft-amazon-ses": "^2.0.1",
    "sebastianlenz/linkfield": "^2.1.5",
    "servd/craft-asset-storage": "^3.3.0",
    "spicyweb/craft-embedded-assets": "^3.1.3",
    "verbb/formie": "^2.0.25.1",
    "verbb/image-resizer": "^3.0.4",
    "verbb/super-table": "^3.0.8.1",
    "vlucas/phpdotenv": "^5.5.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.1",
    "nystudio107/craft-autocomplete": "^1.11.0",
    "yiisoft/yii2-shell": "^2.0.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php -r \"unlink('composer.json');\"",
      "@php -r \"rename('composer.json.default', 'composer.json');\"",
      "@composer dump-autoload -o"
    ]
  }
}
